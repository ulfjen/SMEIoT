{
  "compilerOptions": {
    "outDir": "./wwwroot/dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es2018",
      "dom"
    ],
    "jsx": "react",
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
